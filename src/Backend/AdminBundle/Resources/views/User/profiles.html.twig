<h2>Profil</h2> 
<form action="{{ path('backend_admin_profile_edit',{'userId':userId})}}" method="post" {{ form_enctype(form) }}  class="editProfile">
    <table class="profile_table">
        <tbody>
            <tr>
                <td>{{ form_label(form.name, 'Név:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.name) }}</td>
            </tr>  
            <tr>
                <td>{{ form_label(form.telephone, 'Telefonszám:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.telephone) }}</td>
            </tr>
            <tr>
                <td colspan="2">Számlázási adatok</td>
            </tr>
            <tr>
                <td>{{ form_label(form.billingAddressCity, 'Település:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.billingAddressCity) }}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.billingAddressStreet, 'Útca/út:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.billingAddressStreet) }}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.billingAddressStreetNumber, 'Házszám:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.billingAddressStreetNumber) }}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.billingAddressZipCode, 'Irányítószám:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.billingAddressZipCode) }}</td>
            </tr>
            <tr>
                <td colspan="2">Szállítási adatok</td>
            </tr>
            <tr>
                <td>{{ form_label(form.shippingAddressCity, 'Település:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.shippingAddressCity) }}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.shippingAddressStreet, 'Útca/út:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.shippingAddressStreet) }}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.shippingAddressStreetNumber, 'Házszám:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.shippingAddressStreetNumber) }}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.shippingAddressZipCode, 'Irányítószám:', { 'label_attr': {'class': 'labelText'}}) }}        </td>
                <td>{{ form_widget(form.shippingAddressZipCode) }}</td>
            </tr>
            <tr >
                <td colspan="2">
                    <input type="submit" value="Mentés" class="menu_btn ok_btn">
                    <input type="reset" value="Mégse" class="menu_btn warning_btn cancel_btn">
                </td>
            </tr>
        </tbody>
    </table>            
    {{ form_widget(form._token) }}
</form> 

