<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Frontend\ProductBundle\Entity\ProductProperty"  repository-class="Frontend\ProductBundle\Repository\ProductPropertyRepository" table="product_property">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="value" type="string" column="value" length="200" nullable="false"/>
    <many-to-one field="property" target-entity="Propertys">
      <join-columns>
        <join-column name="property_id" referenced-column-name="id"/>
      </join-columns>

    </many-to-one>
    <many-to-one field="product" target-entity="Product"   inversed-by="productPropertys">
      <join-columns>
        <join-column name="product_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
